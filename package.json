{
  "name": "uic-rider",
  "version": "1.0.0",
  "description": "UIC night ride application",
  "main": "routes/index.js",
  "scripts": {
    "debug": "cross-env DEBUG=express:*,final* node ./bin/www",
    "start": "cross-env DEBUG=final*  node ./bin/www",
    "test": "cross-env NODE_ENV=test mocha",
    "test-for-coverage": "npm test -- -R min || exit 0",
    "style": "npm run test -- test/style.js",
    "coverage": "cross-env NODE_ENV=test nyc --check-coverage=true --reporter=lcov --reporter=text npm run test-for-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckanich-classrooms/final-project-create-table-students.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ckanich-classrooms/final-project-create-table-students/issues"
  },
  "homepage": "https://github.com/ckanich-classrooms/final-project-create-table-students#readme",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "firebase": "^7.9.2",
    "firebase-admin": "^8.9.2",
    "firebaseui": "^4.4.0",
    "hbs": "^4.1.0",
    "morgan": "~1.9.1",
    "stoppable": "^1.1.0"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "axios-cookiejar-support": "^0.5.1",
    "cross-env": "^7.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "tough-cookie": "^3.0.1",
    "nodemon": "^2.0.2"
  },
  "nyc": {
    "branches": 80,
    "lines": 80,
    "functions": 100,
    "statements": 80
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  }
